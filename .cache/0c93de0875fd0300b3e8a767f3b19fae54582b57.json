;\n        }else{\n            this.message.text=null;\n        }\n    \n    });\n\n\n    },\n    markMessageAsRead() {\n        let unReadMessages = [];\n        unReadMessages = this.messages.filter(message => (message.read === false) && (message.idReceiver === Meteor.userId()) );\n        console.log('this.messages List: ', this.messages);\n        console.log('unReadMessages : ', unReadMessages);\n        if(unReadMessages.lenght > 0){\n            Meteor.call('messages.read',unReadMessages,(error) =>{\n                if(error){\n                    this.$alert.showAlertSimple('error',error.reason);\n                }\n    \n            });\n        }\n    }\n},\n  meteor:{\n    $subscribe: {\n      'user.list':[],\n      'message.list': function(){\n          return [this.contactSelected ? this.contactSelected._id : null];\n      }\n    },\n    users(){\n      return Meteor.users.find({_id:{$ne:Meteor.userId()}}).fetch();\n    },\n    messages() {\n        return this.contactSelected ? Message.find({},{\n          sort: {\n            date:1\n          }\n        }).fetch(): [];\n    }\n  }\n};\n"]},"sourceType":"module","hash":"0c93de0875fd0300b3e8a767f3b19fae54582b57"}
